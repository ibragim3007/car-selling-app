{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*", "assets/*"],
    },
    "target": "ES6",
    "lib": ["dom", "esnext"],
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "resolveJsonModule": true,
  },
  "exclude": ["node_modules", "react-native.config.js", "metro.config.js", "babel.config.js"]
}
